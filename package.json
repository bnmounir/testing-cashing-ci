    "author": "",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.635.0",
        "body-parser": "1.19.0",
        "concurrently": "^5.1.0",
        "cookie-session": "^2.0.0-beta.2",
        "dotenv": "^8.2.0",
        "express": "^4.15.3",
        "jest": "^24.9.0",
        "migrate-mongoose": "^4.0.0",
        "mongoose": "^5.9.3",
        "nodemon": "2.0.2",
        "passport": "0.4.1",
        "passport-google-oauth20": "2",
        "path-parser": "6.1.0",
        "puppeteer": "^2.1.1",
        "redis": "^3.0.2",
        "uuid": "^7.0.2"
    },
